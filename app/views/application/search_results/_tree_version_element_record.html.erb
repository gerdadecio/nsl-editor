<tr id="search-result-<%= search_result.id %>" 
  class="search-result  <%= 'fresh' if search_result.fresh? %>"  
  data-record-id="<%#= search_result.id %>"


  data-get-url="<%#= tree_version_element_path(search_result.id) %>"
  data-edit-url="<%#= tree_version_element_path(search_result.id) %>"
  data-base-url="<%#= tree_version_element_path(search_result.id) %>"
  data-tab-url="<%= tree_version_element_tab_path(element_link: search_result[:element_link],tab: 'active_tab_goes_here') %>"
  data-record-id="<%#= search_result.id %>"

  data-record-type="tree"
  tabindex="<%= increment_tab_index(100) %>"
    >
  <td class="nsl-tiny-icon-container takes-focus width-1-percent"><%= record_icon('tree-version-element') %></td>
  <td class="text takes-focus draft_name main-content min-width-40-percent max-width-60-percent width-50-percent <%= 'give-me-focus' if give_me_focus %>">
    <a
      class="show-details-link"
      tabindex="<%= increment_tab_index %>" 
      title="Tree Version Element record. Select to show details."
      id="<%= %Q(#{search_result.class}-#{search_result.id}) %>">
      <%= render partial: 'tree_version_element_link_text', locals: {search_result: search_result} %>
    </a>
  </td>
  <td class="takes-focus width-5-percent filler">&nbsp;</td>
</tr>
