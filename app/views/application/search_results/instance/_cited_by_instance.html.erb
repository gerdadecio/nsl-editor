<tr id="search-result-<%= search_result.id %>" class="search-result cited-by-instance" tabindex="<%= increment_tab_index(100) %>"
    data-get-url="<%= instance_path(search_result.id) %>"
    data-edit-url="<%= instance_path(search_result.id) %>"
    data-base-url="<%= instance_path(search_result.id) %>"
    data-tab-url="<%= instance_tab_path(id: search_result.id, tab: 'active_tab_goes_here') %>"
    data-record-id="<%= search_result.id %>"
    data-record-type="instance"
    data-row-type="cited_by_instance"
    data-instance-type="<%= search_result.type %>"
>
  <td class="nsl-tiny-icon-container width-1-percent"><%= record_icon('instance') %></td>
  <td colspan="4" class="text takes-focus width-80-percent indent-level-1">
    <a id="<%= %Q(#{search_result.class.name}-#{search_result.id}) %>"
       class="indent-level-2 show-details-link <%= "draft" if search_result.draft? %>"
       title="Instance cited by record.  Select to show details."
       tabindex="<%= increment_tab_index %>"
    >
      <%= "#{search_result.expanded_instance_type || search_result.instance_type.name}: #{search_result.name.full_name}" %>
      <%= "#{search_result.page}" %>
      <%= render(partial: 'draft', locals: { search_result: search_result }) %>
    </a>
     <%= render(partial: 'record_debug', locals: { search_result: search_result }) if @debug %>

  </td>
  <td class="takes-focus width-10-percent filler">&nbsp;</td>
</tr>

